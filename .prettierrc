{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"semi": false,
	"printWidth": 100,
	"plugins": [
		"prettier-plugin-astro",
		"prettier-plugin-tailwindcss",
		"prettier-plugin-astro-organize-imports"
	],
	"pluginSearchDirs": false,
	"overrides": [
		{
			"files": "*.astro",
			"options": {
				"parser": "astro"
			}
		}
	]
}
